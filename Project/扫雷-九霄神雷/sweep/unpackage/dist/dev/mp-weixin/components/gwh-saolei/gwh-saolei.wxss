
.content {
		display: flex;
		flex-direction: column;
		align-items: center;
		justify-content: space-between;
		font-family: 楷体;
}
.contentMap {
		max-width: 100vw;
		max-height: 60vh;
		overflow: auto;
		margin-top: 20vh;
		padding: 0 10px;
}
.content.wrap{
		justify-content: start;
		min-height: 80vh;
}
.titleLine{
		width: 90%;
		display: flex;
		flex-direction: row;
		align-items: center;
		justify-content: space-between;
		padding-top: 10px;
		padding-bottom: 10px;
		color: #0000ff;
}
.titleLine image{
		width: 60rpx;
		height: 60rpx;
}
.placeInRow{
		display: flex;
		flex-direction: row;
		justify-content: center;
}
.block{
		width: 82rpx;
		height: 82rpx;
    background-size:100% 100%;
    background-image: url('data:image/jpeg;base64,');
		display: flex;
		flex-direction: column;
		align-items: center;
		justify-content: center;
}
.open {
    font-size: 16px;
    text-align: center;
    font-weight: bold;
    line-height:100px;
    text-transform:uppercase;
    position: relative;
    background-color: #333;
    color:#fff;
    text-shadow:
        0px 1px 0px #c0c0c0,
        0px 2px 0px #b0b0b0,
        0px 3px 0px #a0a0a0,
        0px 4px 0px #909090,
        0px 5px 10px rgba(0, 0, 0, 0.6);
    display: flex;
    align-items: center;
    justify-content: center;
    width: 82rpx;
    height: 82rpx;
    background-size:100% 100%;
    background-image: url('data:image/jpeg;base64,');
}
.boom {
    width: 82rpx;
    height: 82rpx;
    background-size:100% 100%;
    background-image: url("data:image/jpeg;base64,");
}
button.again{
		font-family:  华文新魏 ;
		color:#0000ff ;
		margin-top: 30px;
}
.music { color:#e010c7 ;}
.del {
		text-decoration: line-through;
		color: #ccc;
}
.none{
		border:  solid 42rpx;
		border-image: linear-gradient(202deg,#2cb7a3,#cff3ce)  2 ;
}
image{
		width: 45rpx;
		height: 45rpx;
}

